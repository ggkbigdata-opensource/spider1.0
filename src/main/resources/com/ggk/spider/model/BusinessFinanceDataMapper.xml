<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ggk.spider.mapper.BusinessFinanceDataMapper">

	<insert id="add">
		INSERT INTO tbl_spider_businessfinancedata
		(enterpriseId,enterpriseName,year,financeIndexCode,financeIndexName,financeIndexValue,areListed,srcFileName)
		VALUES
		(#{enterpriseId},#{enterpriseName},#{year},#{financeIndexCode},#{financeIndexName},#{financeIndexValue},#{reportTitle},#{areListed});
	</insert>

</mapper>